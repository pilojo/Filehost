    <servlet>
        <servlet-name>ShareWithGroupServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.ShareWithGroupServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>AddUserToGroupServlet</servlet-name>
        <url-pattern>/server/addUserToGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePermissionServlet</servlet-name>
        <url-pattern>/server/changePermission</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateGroupServlet</servlet-name>
        <url-pattern>/server/createGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteGroupServlet</servlet-name>
        <url-pattern>/server/deleteGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListGroupsServlet</servlet-name>
        <url-pattern>/server/listGroups</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShareWithGroupServlet</servlet-name>
        <url-pattern>/server/shareWithGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchFileServlet</servlet-name>
        <url-pattern>/server/searchFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchUserServlet</servlet-name>
        <url-pattern>/server/searchUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/upload/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/download/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.LoginServlet</servlet-class>
