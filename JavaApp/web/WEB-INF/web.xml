<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <listener>
        <listener-class>com.algonquincollege.waterbin.fs.fsAggregator.FSAggregatorInitializer</listener-class>
    </listener>
    <listener>
        <listener-class>com.algonquincollege.waterbin.fs.transferAggregator.TransferAggregatorInitializer</listener-class>
    </listener>
    <servlet>
        <servlet-name>SearchUserServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.SearchUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchFileServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.SearchFileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUserToGroupServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.AddUserToGroupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePermissionServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.ChangePermissionServlet</servlet-class>
    </servlet>
    <servlet>
