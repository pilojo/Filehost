        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListContentsServlet</servlet-name>
        <servlet-class>com.algonquincollege.javaApp.webhost.servlets.ListContentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/server/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginCheckServlet</servlet-name>
        <url-pattern>/server/isLogedIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveServlet</servlet-name>
        <url-pattern>/server/mv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CopyServlet</servlet-name>
        <url-pattern>/server/cp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveServlet</servlet-name>
        <url-pattern>/server/rename</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveServlet</servlet-name>
        <url-pattern>/server/rm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeDirectoryServlet</servlet-name>
        <url-pattern>/server/mkdir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/server/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListContentsServlet</servlet-name>
        <url-pattern>/server/ls</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/server/signup</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
     <error-page>
       <error-code>404</error-code>
       <location>/index.html</location>
   </error-page>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>Entire Application</web-resource-name>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
</web-app>
